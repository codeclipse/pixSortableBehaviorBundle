{
    "name": "codeclipse/sortable-behavior-bundle",
    "type": "symfony-bundle",
    "description": "Provides a way to sort your admin listing",
    "keywords": ["sortable","admin","sonata"],
    "homepage": "https://github.com/codeclipse/pixSortableBehaviorBundle",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": ">=5.0",
        "symfony/form": ">=5.0"
    },
    "require-dev": {
        "gedmo/doctrine-extensions": ">=3.7"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Can be easily integrated with Sonata Admin bundle",
        "stof/doctrine-extensions-bundle": "1.1.*@dev"
    },
    "conflict": {
        "sonata-project/admin-bundle": "<4.0"
    },
    "autoload": {
        "psr-4": {
            "Codeclipse\\SortableBehaviorBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
